{
   "tool": "HtoA",
   "version": "1.10.0",
   "requires": ["houdini", "HtoAShaders1.10.0"],
   "platforms": ["darwin"],
   "environment":
   {
      "HTOA_ROOT": {"strict": True,
                    "common": "@path/@tool/@version"},
      "HOUDINI_PATH": {"prepend": True,
                       "common": "${HTOA_ROOT}/${HOUDINI_VERSION}"},
      "PATH": {"windows": "${HTOA_ROOT}/${HOUDINI_VERSION}/scripts/bin"}
   }
}